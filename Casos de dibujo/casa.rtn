# Dibuja una casa

program
  setposition(-250, -250);
  forward(300);
  rotater(90);
  forward(300);
  rotater(90);
  forward(300);
  rotatel(-180*7);
  setposition(-500, -250);
  closeeye();
  rotater(90);
  forward(100);
  openeye();
  forward(800);
  closeeye();
  forward(100);
  backward(100);
  rotater(90);
  backward(300);
  rotatel(90);
  backward(350);
  rotatel(120);
  openeye();
  forward(300);
  rotatel(120);
  forward(300);
end;
